<div class="">
  <div class="page-title">
    <div class="title_left">
      <h3><%= t('.page_title') %></h3>
    </div>
  </div>

  <div class="clearfix"></div>

  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <div class="x_panel">
        <div class="x_title">
          <h2><%= t('.report_title') %> <small><%= t('.report_subtitle') %></small></h2>
          <ul class="nav navbar-right panel_toolbox">
            <li>
              <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
              </a>
            </li>
          </ul>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">
          <div class="col-md-3 col-sm-3 col-xs-12 profile_left">
            <div class="profile_img">
              <div id="crop-avatar">
                <img class="img-responsive avatar-view" src="https://placehold.it/220x220" alt="Avatar" title="Change the avatar">
              </div>
            </div>
            <h3><%= current_user.full_name %></h3>

            <ul class="list-unstyled user_data">
              <li>
                <i class="fa fa-user user-profile-icon"></i>
                <%= current_user.dni %>
              </li>
              <li>
                <i class="fa fa-map-marker user-profile-icon"></i>
                <%= current_user.province_name %>
              </li>
            </ul>

            <%= link_to edit_user_registration_path, class: 'btn btn-success' do %>
              <i class="fa fa-edit m-right-xs"></i>
              <%= t('.edit_profile') %>
            <% end %>
            <br>

          </div>
          <div class="col-md-9 col-sm-9 col-xs-12">
            <%= render "#{current_user.type.downcase}_tabs" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
