<p id="notice"><%= notice %></p>

<h1><%= t'inscriptions.index' %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"><%= t'inscriptions.course' %></th>
      <th colspan="3"><%= t'inscriptions.created_at' %></th>
    </tr>
  </thead>

  <tbody>
    <% @inscriptions.each do |inscription| %>
      <tr>
        <td><%= inscription.course.name %></td>
        <td><%= inscription.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t(:back), courses_path %>
